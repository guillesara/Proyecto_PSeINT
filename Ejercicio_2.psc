Proceso Ejercicio_2
	Definir Numero1, Numero2 Como Entero;
	
	Escribir "Ingrese un numero";
	Leer Numero1;
	Escribir "Ingrese otro numero";
	Leer Numero2;
	
	si Numero1 > Numero2 Entonces
		Escribir Numero1, " es mayor que ", Numero2;
	SiNo
		Escribir Numero2, " es mayor que ", Numero1;
	FinSi
FinProceso
